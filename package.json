{
  "version": "0.3.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/use-wait-for-transaction-hash.cjs.production.min.js",
      "import": "./dist/use-wait-for-transaction-hash.esm.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "vercel-build": "cd example && yarn install && yarn build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "use-wait-for-transaction-hash",
  "keywords": [
    "react-hook",
    "wait-for-transaction-hash",
    "eth"
  ],
  "author": "Huynh Duc Dung",
  "repository": {
    "type": "git",
    "url": "https://github.com/jellydn/use-wait-for-transaction-hash"
  },
  "module": "dist/use-wait-for-transaction-hash.esm.js",
  "size-limit": [
    {
      "path": "dist/use-wait-for-transaction-hash.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-wait-for-transaction-hash.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@changesets/cli": "2.22.0",
    "@size-limit/preset-small-lib": "7.0.8",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "husky": "8.0.1",
    "isomorphic-fetch": "3.0.0",
    "msw": "0.41.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "size-limit": "7.0.8",
    "standard-version": "9.5.0",
    "tsdx": "0.14.1",
    "tslib": "2.4.0",
    "typescript": "4.7.2"
  },
  "msw": {
    "workerDirectory": "src"
  }
}
